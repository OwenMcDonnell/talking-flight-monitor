<UserControl x:Class="tfm.Settings_panels.Weather.ctlAutomaticAnnouncements"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:tfm.Settings_panels.Weather"
             xmlns:s="clr-namespace:tfm"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel>
        <CheckBox Content="Use SAPI for cloud announcements" IsChecked="{s:WeatherSettingBinding CloudLayers_UseSAPI}" />

    </WrapPanel>
</UserControl>
